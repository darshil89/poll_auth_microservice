datasource db {
  provider = "mongodb"
  url      = env("AUTH_DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-py"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

model User {
    id String @id @default(auto()) @map("_id") @db.ObjectId
    email String @unique
    name String?
    password String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}